{
  "name": "Drinkr",
  "short_name": "Drinkr",
  "version": "0.3.0",
  "manifest_version": 2,
  "description": "Behold the everday struggle of drinking too little.",
  "author": "Markus &Ouml;lhafen",
  "homepage_url": "http://markusoelhafen.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "omnibox": { "keyword" : "drinkr" },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Drinkr",
    "default_popup": "index.html"
  },
  "permissions": [
    "notifications",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}